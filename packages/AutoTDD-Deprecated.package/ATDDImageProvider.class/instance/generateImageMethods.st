generation
generateImageMethods

	self resourceDictionary keysAndValuesDo: [:key :value |
		(self loadResource: value ifAbsent: [nil]) ifNotNil: [:form |
			self class
				compile: 
					key asString, String cr, String cr, '" autogenerated, do not modify "',
					String cr, '^', form storeString, String cr
				classified: #'- autogenerated -'
				notifying: nil]].